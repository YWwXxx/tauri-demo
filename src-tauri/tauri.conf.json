    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "应用名",
    "version": "0.1.6"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "process": {
        "relaunch": true
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.d",
      "windows": {
        "wix": {
          "language": "zh-CN"
        }
      },
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": false,
      "endpoints": ["https://ywwxxx.github.io/update.json.sig"],
